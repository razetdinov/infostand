						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="partnerCode">
				<xsd:annotation>
					<xsd:documentation>Код дополнительной услуги ТП</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name">
				<xsd:annotation>
					<xsd:documentation>Наименование дополнительной услуги</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="description" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание дополнительной услуги</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="512"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="amount" type="cmn:AmountType">
				<xsd:annotation>
					<xsd:documentation>Цена</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="currency" type="cmn:CurrencyType">
				<xsd:annotation>
					<xsd:documentation>Буквенный код валюты ISO 4217, условная валюта.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FindProductsRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос поиска товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="categoryCode">
				<xsd:annotation>
					<xsd:documentation>Код категории товаров</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="parameters" type="tns:FindParameterListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Cписок параметров товаров для поиска.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blockNumber" type="cmn:BlockNumberType" default="1">
				<xsd:annotation>
					<xsd:documentation>Номер блока записей (в каждом блоке по 100 записей)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FindParameterListType">
		<xsd:annotation>
			<xsd:documentation>Список параметров товаров для поиска</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="parameter" type="tns:FindParameterType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Список параметров товаров для поиска</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FindParameterType">
		<xsd:annotation>
			<xsd:documentation>Информация о параметре для поиска товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="parameterName">
				<xsd:annotation>
					<xsd:documentation>Наименование параметра товара</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="restriction" type="tns:ProdCharacteristicRestrictionType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Ограничение параметра товара для поиска.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FindProductsResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос поиска товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:FindProductsResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса поиска товаров</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FindProductsResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса поиска товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="products" type="tns:ProductExportListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Cписок выгружаемых товаров. Отсутствует, если получение информации по товарам не удалось по причине ошибки, детализированной в result или при отсутствии товаров.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductExportListType">
		<xsd:annotation>
			<xsd:documentation>Cписок выгружаемых товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="product" type="tns:ProductExportShortType" maxOccurs="100">
				<xsd:annotation>
					<xsd:documentation>Список товаров, блок размером 100 записей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="program" type="tns:ProgramType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="category" type="tns:CategoryType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Информация о категории</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductShortType">
		<xsd:annotation>
			<xsd:documentation>Краткая информация о товаре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name">
				<xsd:annotation>
					<xsd:documentation>Наименование товара</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="description" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание товара</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="512"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="amount" type="cmn:AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Цена</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="currency" type="cmn:CurrencyType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Буквенный код валюты ISO 4217, условная валюта.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductExportShortType">
		<xsd:annotation>
			<xsd:documentation>Краткая информация о выгружаемом товаре</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ProductShortType">
				<xsd:sequence>
					<xsd:element name="opcCode">
						<xsd:annotation>
							<xsd:documentation>Код товара ОПЦ</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="36"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="picture" type="cmn:ImageType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Изображение товара</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="RegisterOrderRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос регистрации заказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="orderInfo" type="tns:RegisterOrderInfoType">
				<xsd:annotation>
					<xsd:documentation>Информация о заказе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ds:Signature">
				<xsd:annotation>
					<xsd:documentation>Электронная подпись транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegisterOrderResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос регистрации заказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:RegisterOrderResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса регистрации заказа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegisterOrderInfoType">
		<xsd:annotation>
			<xsd:documentation>Информация о заказе при регистрации</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="partnerOrderId">
				<xsd:annotation>
					<xsd:documentation>Код заказа в системе Партнера</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="contactName">
				<xsd:annotation>
					<xsd:documentation>Контактное лицо</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="phoneNumber">
				<xsd:annotation>
					<xsd:documentation>Контактный телефон</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="email" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контактный электронный адрес</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
						<xsd:pattern value="(\w[-._\w]*\w@\w[-._\w]*\w\.\w{2,10})"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="deliveryAddress" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес доставки</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="512"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="deliveryDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата доставки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegisterOrderResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса регистрации заказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="orderId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код заказа в ОПЦ. Отсутствует, если регистрация заказа не удалась по причине ошибки, детализированной в result.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetOrderStatusRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос получения состояния заказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="orderId">
				<xsd:annotation>
					<xsd:documentation>Код заказа в ОПЦ</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetOrderStatusResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения состояния заказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:GetOrderStatusResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса получения состояния заказа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetOrderStatusResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения состояния заказа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="order" type="tns:OrderType" minOccurs="0">
