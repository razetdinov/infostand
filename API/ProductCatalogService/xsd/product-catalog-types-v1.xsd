<xsd:schema xmlns:tns="http://schemas.opc.bssys.com/product/catalog/service/types/v1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:cmn="http://schemas.opc.bssys.com/product/catalog/service/common/v1" targetNamespace="http://schemas.opc.bssys.com/product/catalog/service/types/v1" elementFormDefault="qualified">
	<xsd:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd"/>
	<xsd:import namespace="http://schemas.opc.bssys.com/product/catalog/service/common/v1" schemaLocation="product-catalog-common-v1.xsd"/>
	<xsd:complexType name="GetProgramsRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос получения списка программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blockNumber" type="cmn:BlockNumberType" default="1">
				<xsd:annotation>
					<xsd:documentation>Номер блока записей (в каждом блоке по 100 записей)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProgramsResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения списка программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:GetProgramsResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса получения списка программ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProgramsResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения списка программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="programs" type="tns:ProgramListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Cписок программ. Отсутствует, если получение информации по списку программ не удалось по причине ошибки, детализированной в result или при отсутствии программ.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProgramListType">
		<xsd:annotation>
			<xsd:documentation>Список программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="program" type="tns:ProgramType" maxOccurs="100">
				<xsd:annotation>
					<xsd:documentation>Список программ, блок размером 100 записей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProgramType">
		<xsd:annotation>
			<xsd:documentation>Информация о программе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="code">
				<xsd:annotation>
					<xsd:documentation>Код программы</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name">
				<xsd:annotation>
					<xsd:documentation>Наименование программы</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="mixedCheck" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак возможности использования смешанного чека по программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetCategoriesRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос получения списка категорий и характеристик по программе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="withCharacteristics" type="xsd:boolean" default="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак получения характеристик категорий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="programCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код программы, в случае, если не указан, то запрос получения категорий, не принадлежащих программам.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="blockNumber" type="cmn:BlockNumberType" default="1">
				<xsd:annotation>
					<xsd:documentation>Номер блока записей (в каждом блоке по 100 записей)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetCategoriesResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения списка категорий и характеристик по программе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:GetCategoriesResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса получения списка категорий и характеристик по программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetCategoriesResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения списка категорий и характеристик по программе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="categories" type="tns:CategoryListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Cписок категорий. Отсутствует, если получение информации по списку категорий и характеристик не удалось по причине ошибки, детализированной в result или при отсутствии категорий.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CategoryListType">
		<xsd:annotation>
			<xsd:documentation>Список категорий</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="program" type="tns:ProgramType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="category" type="tns:CategoryWithCharType" maxOccurs="100">
				<xsd:annotation>
					<xsd:documentation>Список категорий, блок размером 100 записей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CategoryType">
		<xsd:annotation>
			<xsd:documentation>Информация о категории</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="code">
				<xsd:annotation>
					<xsd:documentation>Код категории</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name">
				<xsd:annotation>
					<xsd:documentation>Наименование категории</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CategoryWithCharType">
		<xsd:annotation>
			<xsd:documentation>Информация о категории с характеристиками</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:CategoryType">
				<xsd:sequence>
					<xsd:element name="characteristics" type="tns:CharacteristicListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Cписок характеристик. Отсутствует, если в запросе получения списка категорий и характеристик признак withCharacteristics=false или при отсутствии характеристик.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CharacteristicListType">
		<xsd:annotation>
			<xsd:documentation>Список характеристик категории</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="characteristic" type="tns:CharacteristicType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Список характеристик категории</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CharacteristicType">
		<xsd:annotation>
			<xsd:documentation>Информация о характеристике категории</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name">
				<xsd:annotation>
					<xsd:documentation>Наименование характеристики</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="type" type="cmn:CharacteristicTypeCode">
				<xsd:annotation>
					<xsd:documentation>Тип характеристики категории</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="restriction" type="tns:ProdCharacteristicRestrictionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ограничение характеристики категории. Отсутствует, если нет ограничений.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProdCharacteristicRestrictionType">
		<xsd:annotation>
			<xsd:documentation>Ограничение характеристики категории или параметра товара</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="stringRestrictionValue">
				<xsd:annotation>
					<xsd:documentation>Строковое ограничение</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="2000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="booleanRestrictionValue">
				<xsd:annotation>
					<xsd:documentation>Логическое ограничение</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="numberRestrictionValue" type="tns:NumberRestrictionType">
				<xsd:annotation>
					<xsd:documentation>Числовое ограничение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="stringArrayRestrictionValue" type="tns:StringArrayRestrictionType">
				<xsd:annotation>
					<xsd:documentation>Массив строковых ограничений</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="NumberRestrictionType">
		<xsd:annotation>
			<xsd:documentation>Числовое ограничение характеристики</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="numberFrom" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Нижняя граница</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="numberTo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Верхняя граница</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal"/>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="StringArrayRestrictionType">
		<xsd:annotation>
			<xsd:documentation>Массив строковых ограничений характеристики</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="stringValue" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Строковое ограничение</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="2000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProductRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос получения товара</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="productCode">
				<xsd:annotation>
					<xsd:documentation>Код товара ОПЦ</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProductResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения товара</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:GetProductResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса получения товара</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProductResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения товара</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="product" type="tns:ProductExportType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Товар. Отсутствует, если получение информации по товару не удалось по причине ошибки, детализированной в result.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductExportType">
		<xsd:annotation>
			<xsd:documentation>Информация о выгружаемом товаре</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ProductShortType">
				<xsd:sequence>
					<xsd:element name="opcCode">
						<xsd:annotation>
							<xsd:documentation>Код товара ОПЦ</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="36"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="partnerCode" type="cmn:PartnerProductCodeType">
						<xsd:annotation>
							<xsd:documentation>Код товара ТП</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="parameters" type="tns:ProductParameterListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Характеристики товара. Отсутствует, если нет характеристик.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="services" type="tns:ProductServiceListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дополнительные услуги к товару. Отсутствует, если нет дополнительных услуг.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="processingInfo" type="tns:ProcessingChooseType">
						<xsd:annotation>
							<xsd:documentation>Информация о точках продажи для процессинга ОПЦ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="program" type="tns:ProgramType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Информация о программе</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="category" type="tns:CategoryType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Информация о категории</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="pictures" type="cmn:ImageListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Изображения товара</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ProcessingInfoType">
		<xsd:annotation>
			<xsd:documentation>Информация для процессинга ОПЦ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="partner" type="tns:PartnerType">
				<xsd:annotation>
					<xsd:documentation>Партнер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="merchant" type="tns:MerchantType">
				<xsd:annotation>
					<xsd:documentation>Поставщик услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="poi" type="tns:PoiType">
				<xsd:annotation>
					<xsd:documentation>Терминал</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProcessingChooseType">
		<xsd:annotation>
			<xsd:documentation>Информация о точках продажи для процессинга ОПЦ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="partner" type="tns:PartnerType">
				<xsd:annotation>
					<xsd:documentation>Партнер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="merchants" type="tns:MerchantWithPoisListType">
				<xsd:annotation>
					<xsd:documentation>Поставщики</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductParameterListType">
		<xsd:annotation>
			<xsd:documentation>Список характеристик товара</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="parameter" type="tns:ProductParameterType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Список характеристик товара</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductServiceListType">
		<xsd:annotation>
			<xsd:documentation>Список дополнительных услуг к товару</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="service" type="tns:ProductServiceType" maxOccurs="unbounded">
				<xsd:annotation>
