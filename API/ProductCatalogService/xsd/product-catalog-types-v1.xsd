					<xsd:documentation>Список товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TxProductType">
		<xsd:annotation>
			<xsd:documentation>Информация о товаре или услуге</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="partnerCode">
				<xsd:annotation>
					<xsd:documentation>Код товара или услуги ТП</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name">
				<xsd:annotation>
					<xsd:documentation>Наименование</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="500"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="amount" type="cmn:AmountType">
				<xsd:annotation>
					<xsd:documentation>Цена</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="currency" type="cmn:CurrencyType">
				<xsd:annotation>
					<xsd:documentation>Буквенный код валюты ISO 4217, условная валюта.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Количество единиц</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="17"/>
						<xsd:totalDigits value="18"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="unitPrice" type="cmn:AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Цена единицы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="program" type="tns:ProgramType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="category" type="tns:CategoryType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о категории</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetApprovedProductsRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос получения утвержденных товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="categoryCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код категории товаров, в случае, если не указана, то запрос получения всех утвержденных товаров.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="70"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="blockNumber" type="cmn:BlockNumberType" default="1">
				<xsd:annotation>
					<xsd:documentation>Номер блока записей (в каждом блоке по 100 записей)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetApprovedProductsResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения утвержденных товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:GetApprovedProductsResultType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса получения утвержденных товаров</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetApprovedProductsResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения утвержденных товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="productCodes" type="tns:ProductCodeList" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Список идентификаторов утвержденных товаров. Отсутствует, если получение информации по заказу не удалось по причине ошибки, детализированной в result или при отсутствии утвержденных товаров.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductCodeList">
		<xsd:annotation>
			<xsd:documentation>Список идентификаторов товаров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="code" type="cmn:PartnerProductCodeType" maxOccurs="100">
				<xsd:annotation>
					<xsd:documentation>Список идентификаторов, блок размером 100 записей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImportCatalogRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос импорта каталога товаров и услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="importCatalog" type="tns:ImportCatalogType">
				<xsd:annotation>
					<xsd:documentation>Информация о каталоге товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImportCatalogResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос импорта каталога товаров и услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса импорта каталога товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImportCatalogType">
		<xsd:annotation>
			<xsd:documentation>Информация о каталоге товаров и услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="products" type="tns:ProductImportListType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Cписок загружаемых товаров, услуг, доп. услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProductCatalogStatusRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос получения статуса импорта каталога товаров и услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestUUID" type="cmn:UUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор запроса импорта каталога товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProductCatalogStatusResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения статуса импорта каталога товаров и услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="getProductCatalogStatusResult" type="tns:GetProductCatalogStatusResultType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetProductCatalogStatusResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения статуса импорта каталога товаров и услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="status" type="cmn:ResultResponseType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Результат обработки запроса импорта каталога товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="actualUUID" type="cmn:UUID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор запроса импорта актуального каталога товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductImportListType">
		<xsd:annotation>
			<xsd:documentation>Cписок загружаемых товаров, услуг, доп. услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="product" type="tns:ProductImportType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Загружаемый товар, услуга, доп. услуга</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductImportType">
		<xsd:annotation>
			<xsd:documentation>Информация о загружаемом товаре, услуге, доп. услуги</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ProductShortType">
				<xsd:sequence>
					<xsd:element name="productType" type="cmn:ProductType">
						<xsd:annotation>
							<xsd:documentation>Тип продукта</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="partnerCode" type="cmn:PartnerProductCodeType">
						<xsd:annotation>
							<xsd:documentation>Код товара/услуги ТП</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="category" type="tns:CategoryType">
						<xsd:annotation>
							<xsd:documentation>Информация о категории</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="parameters" type="tns:ProductParameterListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Характеристики товара/услуги.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="pictures" type="cmn:ImageListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Изображения товара/услуги</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="GetPartnersRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос на получение списка поставщиков по социальным программам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="programCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код программы, в случае, если не указан, то запрос получения поставщиков по всем соц. программам.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="blockNumber" type="cmn:BlockNumberType" default="1">
				<xsd:annotation>
					<xsd:documentation>Номер блока записей (в каждом блоке по 100 записей)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetPartnersResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос получения списка поставщиков по социальным программам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="responseHeader" type="cmn:HeaderType">
				<xsd:annotation>
					<xsd:documentation>Заголовок ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="requestHeader" type="cmn:HeaderType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Заголовок запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="responseResult" type="tns:GetPartnersResultType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetPartnersResultType">
		<xsd:annotation>
			<xsd:documentation>Результат обработки запроса получения списка поставщиков по соц. программам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result" type="cmn:ResultResponseType"/>
			<xsd:element name="partners" type="tns:PartnerListType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PartnerListType">
		<xsd:annotation>
			<xsd:documentation>Список поставщиков в разрезе социальных программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="partnerWithPrograms" type="tns:PartnerWithProgramsType" maxOccurs="100"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PartnerWithProgramsType">
		<xsd:annotation>
			<xsd:documentation>Информация о поставщике в разрезе соц. программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="merchantInfo" type="tns:ProcessingInfoType">
				<xsd:annotation>
					<xsd:documentation>Информация о поставщике услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="programs" type="tns:ProgramPartnerListType">
				<xsd:annotation>
					<xsd:documentation>Список программ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProgramPartnerListType">
		<xsd:annotation>
			<xsd:documentation>Список программ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="program" type="tns:ProgramType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Список социальных программ поставщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PartnerType">
		<xsd:annotation>
			<xsd:documentation>Информация о партнере</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id">
				<xsd:annotation>
					<xsd:documentation>Идентификатор</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="11"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="shortName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MerchantType">
		<xsd:annotation>
			<xsd:documentation>Информация о поставщике услуг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id">
				<xsd:annotation>
					<xsd:documentation>Идентификатор</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="shortName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PoiType">
		<xsd:annotation>
			<xsd:documentation>Информация о терминале</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
