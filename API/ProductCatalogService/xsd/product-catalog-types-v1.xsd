					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="8"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="shortName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MerchantWithPoisListType">
		<xsd:annotation>
			<xsd:documentation>Список поставщиков с терминалами</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="merchant" type="tns:MerchantWithPoisType" maxOccurs="unbounded"/>
