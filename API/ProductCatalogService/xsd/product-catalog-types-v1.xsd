					<xsd:documentation>Поставщик</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pois" type="tns:PoiListType">
				<xsd:annotation>
					<xsd:documentation>Терминалы поставшика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PoiListType">
		<xsd:annotation>
			<xsd:documentation>Список терминалов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="poi" type="tns:PoiType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
